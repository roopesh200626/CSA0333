#include <stdio.h>
void main() {
    int arr[] = {5, 10, 15, 20, 25};
    int n = 5, key = 20, found = 0;

    for(int i = 0; i < n; i++) {
        if(arr[i] == key) {
            printf("Element %d found at index %d\n", key, i);
            found = 1;
            break;
        }
    }

    if(!found)
        printf("Element %d not found\n", key);
}
